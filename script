-- Crear secciones
local sections = {
    {Name = "General", Desc = "Configuraci√≥n general del menu", Icon = "‚öôÔ∏è"},
    {Name = "World", Desc = "Ajustes del entorno y mundo", Icon = "üåé"},
    {Name = "Main", Desc = "Funciones principales del script", Icon = "üöÄ"},
